= form_for @factory_manufacturing_process do |f|
  - if @factory_manufacturing_process.errors.any?
    #error_explanation
      %h2= "#{pluralize(@factory_manufacturing_process.errors.count, "error")} prohibited this factory_manufacturing_process from being saved:"
      %ul
        - @factory_manufacturing_process.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :factory_id
    = f.number_field :factory_id
  .field
    = f.label :manufacturing_process_id
    = f.number_field :manufacturing_process_id
  .actions
    = f.submit 'Save'
